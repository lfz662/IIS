@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    string pageName = Convert.ToString(Umbraco.Field("seoTitle", altFieldAlias: "pageName"));
    string pageDescriptions = Convert.ToString(Umbraco.Field("seoMetaDescription", recursive: true));
    string keyWord = Convert.ToString(Umbraco.Field("seoMetaKeywords", recursive: true));
     string[] coolection = CurrentPage.seoMetaKeywords;//CurrentPage.getPropertyValue("seoMetaKeywords").Value.ToList();
    var result = string.Join(",", coolection);
}
@{
    <title>@pageName</title>
    <meta name="description" content="@pageDescriptions">
    <meta name="keywords" content="@result">
